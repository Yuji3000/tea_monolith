   <% if logged_in? %>
      <%= link_to "Logout", sessions_path, method: :delete %>
  <% end %>


  <%= link_to "Teas", teas_path, method: :get %>

 
 <% if current_user != nil%>
  <%= link_to "My Subscriptions", "/users/#{current_user.id}/subscriptions", method: :get %>
<% end %>

<h1>Your Subscriptions</h1>

<% if @user.subscriptions.present? %>
  <% @user.list_teas.map do |tea|%>
    <%= tea.title%><br>
    <p>$<%= tea.price_usd%></p><br>
  <% end %>
<% else %>
  <p>No Subscriptions yet! </p>
  <% end %>